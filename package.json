{
  "name": "rtmdbfe",
  "version": "0.0.2",
  "scripts": {
    "dev": "$(npm bin)/webpack-dev-server --open",
    "dev:details": "npm run dev -s -- --env.details",
    "prebuild": "rm -rf ./dist",
    "build": "NODE_ENV=production $(npm bin)/webpack --progress",
    "build:details": "npm run build -s -- --env.details",
    "lint": "$(npm bin)/tslint -p .",
    "compile": "$(npm bin)/tsc --noEmit",
    "test": "npm run lint -s && npm run compile -s && jest --env=jsdom --coverage",
    "test:watch": "jest --env=jsdom --watch",
    "start": "npm run dev -s"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/types.ts",
      "!src/**/index.ts",
      "!src/reducers/root.ts",
      "!src/index.tsx"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.test.json"
      }
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/setup/enzyme.js",
      "<rootDir>/config/jest/setup/localStorage.js",
      "<rootDir>/config/jest/setup/fetch.js"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/**/?(*.)spec.ts?(x)"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.scss$": "identity-obj-proxy",
      "^.+\\.tsx?$": "ts-jest",
      "^(?!.*\\.(js|jsx|scss|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ]
  },
  "dependencies": {
    "classnames": "2.2.6",
    "normalize.css": "8.0.1",
    "react": "16.6.3",
    "react-dom": "16.6.3",
    "react-redux": "5.1.1",
    "react-redux-toastr": "7.4.1",
    "react-router-dom": "4.3.1",
    "react-transition-group": "2.5.0",
    "redux": "4.0.1",
    "redux-form": "7.4.2",
    "redux-promise-middleware": "5.1.1",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@types/classnames": "2.2.6",
    "@types/deep-freeze": "0.1.1",
    "@types/jest": "23.3.9",
    "@types/react": "16.7.6",
    "@types/react-dom": "16.0.9",
    "@types/react-redux": "6.0.9",
    "@types/react-redux-toastr": "7.4.0",
    "@types/react-router-dom": "4.3.1",
    "@types/react-transition-group": "2.0.14",
    "@types/redux": "3.6.0",
    "@types/redux-form": "7.4.12",
    "@types/redux-immutable-state-invariant": "2.0.4",
    "@types/redux-promise-middleware": "0.0.11",
    "@types/redux-thunk": "2.1.0",
    "autoprefixer": "9.3.1",
    "awesome-typescript-loader": "5.2.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "css-loader": "1.0.1",
    "deep-freeze": "0.0.1",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.0",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "23.6.0",
    "jest-fetch-mock": "1.7.4",
    "mini-css-extract-plugin": "0.4.4",
    "mock-local-storage": "1.1.7",
    "node-sass": "4.10.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "react-dev-utils": "6.1.1",
    "react-test-renderer": "16.6.3",
    "redux-immutable-state-invariant": "2.1.0",
    "sass-loader": "7.1.0",
    "source-map-loader": "0.2.4",
    "style-loader": "0.23.1",
    "ts-jest": "23.10.4",
    "tslint": "5.11.0",
    "tslint-loader": "3.5.4",
    "tslint-react": "3.6.0",
    "typescript": "3.1.6",
    "webpack": "4.26.0",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.10"
  },
  "license": "MIT",
  "author": "gmirzoev"
}
