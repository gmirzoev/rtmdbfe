{
  "name": "rtmdbfe",
  "version": "0.0.2",
  "scripts": {
    "dev": "$(npm bin)/webpack-dev-server --open",
    "dev:details": "npm run dev -s -- --env.details",
    "prebuild": "rm -rf ./dist",
    "build": "NODE_ENV=production $(npm bin)/webpack --progress",
    "build:details": "npm run build -s -- --env.details",
    "lint": "$(npm bin)/tslint -p .",
    "compile": "$(npm bin)/tsc --noEmit",
    "test": "npm run lint -s && npm run compile -s && jest --env=jsdom --coverage",
    "test:watch": "jest --env=jsdom --watch",
    "start": "npm run dev -s"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/types.ts",
      "!src/**/index.ts",
      "!src/reducers/root.ts",
      "!src/index.tsx"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.test.json"
      }
    },
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "setupFiles": [
      "<rootDir>/config/jest/setup/enzyme.js",
      "<rootDir>/config/jest/setup/localStorage.js",
      "<rootDir>/config/jest/setup/fetch.js"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/**/?(*.)spec.ts?(x)"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.scss$": "identity-obj-proxy",
      "^.+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      "^(?!.*\\.(js|jsx|scss|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$"
    ]
  },
  "dependencies": {
    "classnames": "2.2.5",
    "normalize.css": "8.0.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-redux": "5.0.7",
    "react-redux-toastr": "7.2.3",
    "react-router-dom": "4.2.2",
    "react-transition-group": "2.2.1",
    "redux": "3.7.2",
    "redux-form": "7.2.3",
    "redux-promise-middleware": "5.0.0",
    "redux-thunk": "2.2.0"
  },
  "devDependencies": {
    "@types/classnames": "2.2.3",
    "@types/deep-freeze": "0.1.1",
    "@types/jest": "22.1.4",
    "@types/react": "16.0.39",
    "@types/react-dom": "16.0.4",
    "@types/react-redux": "5.0.15",
    "@types/react-redux-toastr": "7.0.10",
    "@types/react-router-dom": "4.2.4",
    "@types/react-transition-group": "2.0.7",
    "@types/redux": "3.6.0",
    "@types/redux-form": "7.0.14",
    "@types/redux-immutable-state-invariant": "2.0.4",
    "@types/redux-promise-middleware": "0.0.9",
    "@types/redux-thunk": "2.1.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "css-loader": "0.28.10",
    "deep-freeze": "0.0.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "1.1.10",
    "html-webpack-plugin": "3.0.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "22.4.2",
    "jest-fetch-mock": "1.4.2",
    "mock-local-storage": "1.0.5",
    "node-sass": "4.7.2",
    "postcss-flexbugs-fixes": "3.3.0",
    "postcss-loader": "2.1.1",
    "react-dev-utils": "5.0.0",
    "react-test-renderer": "16.2.0",
    "redux-immutable-state-invariant": "2.1.0",
    "sass-loader": "6.0.6",
    "source-map-loader": "0.2.3",
    "style-loader": "0.20.2",
    "ts-jest": "22.4.0",
    "ts-loader": "4.0.0",
    "tslint": "5.9.1",
    "tslint-loader": "3.6.0",
    "tslint-react": "3.5.1",
    "typescript": "2.7.2",
    "webpack": "4.0.1",
    "webpack-bundle-analyzer": "2.11.1",
    "webpack-cli": "2.0.9",
    "webpack-dev-server": "3.1.0"
  },
  "license": "MIT",
  "author": "gmirzoev"
}
